





#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int n ,str_temp= 0,i,j,half,t,num;
 scanf("%d",&num);   
for(t =1;t<num+1;t++)
{
    
    scanf("%d",&n);
    printf("Case #%d:\n",t);
    for(i=0;i<n;i++)
    { 
     str_temp = 1;
    
     while(str_temp < n + 1) 
     {
        
        if(i<=n/2)
        {
            if( ((n/2 - i + 1) == str_temp) || ((n/2 + i + 1) == str_temp))
            {
            printf("*");
            }
       
            else
            {
            printf(" ");
            }        
        }
     
     
        else
        {
            if(i == n-1)
            {
                if( (n/2  + 1) == str_temp) 
            {
            printf("*");
            }
       
            else
            {
            printf(" ");
            }       
            }
            else
            {
              if( ((i - n/2 + 1) == str_temp) || ((n - (i%(n/2))) == str_temp))
            {
            printf("*");
            }
       
            else
            {
            printf(" ");
            }        
            } 
        }
      
       str_temp++;
       
      }
      printf("\n");
    
    }
    

}    
    }
    
    


 
